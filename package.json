{
  "name": "orestaurant",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "update ": "npm update",
    "start": "ng build --output-hashing=all --optimization && ng serve",
    "build": "ng build",
    "build4publishUbuntu": "ng build --output-hashing=all --optimization --base-href /restaurants-sanitation-reports-site/ && cp src/index.html docs/404.html ",
    "build4publishWindows": "ng build --output-hashing=all --optimization --base-href /restaurants-sanitation-reports-site/ &&  copy src\\index.html docs\\404.html ",
    "publish": "git add . && git commit -m \"publis version\" && git push ",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "dev:ssr": "ng run orestaurant:serve-ssr",
    "serve:ssr": "node dist/orestaurant/server/main.js",
    "build:ssr": "ng build && ng run orestaurant:server",
    "prerender": "ng run orestaurant:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.3",
    "@angular/cdk": "^15.0.2",
    "@angular/common": "^15.0.3",
    "@angular/compiler": "^15.0.3",
    "@angular/core": "^15.0.3",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.0.3",
    "@angular/material": "^15.0.2",
    "@angular/platform-browser": "^15.0.3",
    "@angular/platform-browser-dynamic": "^15.0.3",
    "@angular/platform-server": "^15.0.3",
    "@angular/router": "^15.0.3",
    "@angular/service-worker": "^15.0.3",
    "@nguniversal/express-engine": "^15.1.0",
    "express": "^4.15.2",
    "firebase": "^9.4.0",
    "material": "^0.4.3",
    "ng2-search-filter": "^0.5.1",
    "npm": "^8.12.1",
    "rxfire": "^6.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular-eslint/builder": "15.1.0",
    "@angular-eslint/eslint-plugin": "15.1.0",
    "@angular-eslint/eslint-plugin-template": "15.1.0",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "^15.0.3",
    "@angular/compiler-cli": "^15.0.3",
    "@nguniversal/builders": "^15.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.46",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "cypress": "^12.3.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~3.10.0",
    "jest": "^27.5.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.8.4"
  }
}